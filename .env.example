# Supabase Public Keys (safe to expose to the browser)
NEXT_PUBLIC_SUPABASE_URL="your-supabase-url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"

# Supabase Service Role Key (NEVER expose to the browser)
# Used for admin-level operations on the server.
SUPABASE_SERVICE_ROLE_KEY="your-supabase-service-role-key"

# Google AI API Key for Genkit
# Used for all AI-powered features.
GEMINI_API_KEY="your-gemini-api-key"

# Paytm Merchant Keys for Payment Gateway
# Use STAGING keys for development/testing and PROD keys for production.
NEXT_PUBLIC_PAYTM_MID="your-paytm-merchant-id"
PAYTM_MERCHANT_KEY="your-paytm-merchant-key"
PAYTM_WEBSITE="WEBSTAGING" # Use "DEFAULT" for production

# Base URL of your deployed application
# Required for payment callbacks and other absolute URLs.
NEXT_PUBLIC_BASE_URL="http://localhost:9002"
